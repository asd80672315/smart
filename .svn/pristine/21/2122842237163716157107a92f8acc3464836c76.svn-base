html,body {
	font-size: 62.5%;
	padding: 0;
	margin: 0;
  height:100%;
  width: 100%;
  font-family:Arial,'Times New Roman','Microsoft YaHei',SimHei;
}

a:hover,
a:focus {
    text-decoration: none;	/* 没有下划线 */
}

/*========index.jsp========*/
.mainSection{
  background: url(/smart/static/img/1.jpg) no-repeat top fixed;
  overflow:hidden;
  -webkit-background-size: cover; 
  -moz-background-size: cover; 
  -o-background-size: cover; 
  background-size: cover;
  /*
  * 一个对象高度是否可以使用百分比显示，取决于对象的父级对象。
  * auto是随内容的高度而撑开的。100%是根据父级元素的高度来决定的。
  */
  height: 100%;
  width: 100%;
}

.mainSection #rowmainSectionHeader{
	margin-top:15%;
}
.mainSection #rowmainSectionHeader h1 { 
  font-size: 9em;  
  font-weight: 6me; 
  color: #fff;
  text-align: center;
}

.mainSection #rowmainSectionHeader p { 
  font-size: 4em; 
  font-weight: 3em; 
  color: #fefefe;
  text-align: center;
}

.mainSection #rowmainSectionButton{
	margin-top:6%;
	text-align:center;
}
.mainSection #rowmainSectionButton .btnmainSection{
  background: rgba(61, 201, 179, 1); 
  border-radius: 5px; 
  -moz-border-radius: 5px; 
  -webkit-border-radius: 5px; 
  -o-border-radius: 5px; 
  color: #fff; 
  text-transform: uppercase; 
  letter-spacing: 1px; 
  display: inline-block; 
  -webkit-box-shadow: 0px 3px 0px 0px #309383; 
  -moz-box-shadow: 0px 3px 0px 0px #309383; 
  box-shadow: 0px 3px 0px 0px #309383; 
  font-weight: bold; 
}

.mainSection #rowmainSectionButton .btnmainSection:hover, .mainSection #rowmainSectionButton .btnmainSection:focus { 
  background: rgba(61, 201, 179, 0.6); 
  color: #fff; 
}

/*!========index.jsp========*/

