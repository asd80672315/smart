/* ----- Navigator ----- */
.navbar{
	z-index: 9999;
  background-color:rgba(76,94,108,0.8);
  opacity:0.8;
  filter:Alpha(opacity=50);/* IE8 以及更早的浏览器 */
	box-shadow:0px 0px 20px rgba(0, 0, 0, 0.1);
  height:6em;
}

.navbar .navbar-header .navbar-brand{
  padding-left: 5em;
}

.navbar .navbar-header .navbar-brand img {
	width:auto;
  height:2.5em;
}

.navbar .collapse ul li a{
	font-size: 3em;
  color: #fff;
	text-transform: Capitalize;
  line-height:2.5em;
	font-weight: 700;
}

.navbar .collapse ul li a:hover{
  background-color:rgba(76,94,108,0);
}


.navbar-right{
  padding-right: 5%;
}

/*! ----- Navigator ----- */

/* ----- Registration Form ----- */

strong { font-weight: 700; }
.red { color: #00bc8f; }

.register{
  margin-top:71px;
  margin-bottom: 10px;
  width:600px;
}

.register .formRegister {
	padding: 10px 30px 20px 30px;
	background: #f8f8f8 url(/smart/static/img/pattern.jpg) left top repeat;
	-moz-border-radius: 8px;
	-webkit-border-radius: 8px;
	border-radius: 8px;
	-moz-box-shadow: 0 1px 5px 0 rgba(0,0,0,.3);
	-webkit-box-shadow: 0 1px 5px 0 rgba(0,0,0,.3);
	box-shadow: 0 1px 5px 0 rgba(0,0,0,.3);
	text-align: center;
	font-size:2.4em;
}

.register .formRegister h2 {
  margin-bottom: 10px;
  padding-bottom: 20px;
  background: url(/smart/static/img/pencil.png) 70px 6px no-repeat;
  border-bottom: 2px dotted #bbb;
  font-size: 2.3em;
  font-weight: 400;
  text-align: center;
}
.register .formRegister .form-group{
  margin-top: 5%; 
}

.register .formRegister label span {
	font-style: italic;
}

.register .formRegister input:-moz-placeholder { color: #aaa; }
.register .formRegister input:-ms-input-placeholder { color: #aaa; }
.register .formRegister input::-webkit-input-placeholder { color: #aaa; }

.register .formRegister button {
    cursor: pointer;
    width: 60%;
    height: 42px;
    margin-top: 25px;
    padding: 0;
    background: #4b5e6c;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    border: 0;
    -moz-box-shadow: 0 15px 30px 0 rgba(255,255,255,.1) inset;
    -webkit-box-shadow: 0 15px 30px 0 rgba(255,255,255,.1) inset;
    box-shadow: 0 15px 30px 0 rgba(255,255,255,.1) inset;
    text-shadow: 0 1px 2px rgba(0,0,0,.1);
    -o-transition: all .2s;
    -moz-transition: all .2s;
    -webkit-transition: all .2s;
    -ms-transition: all .2s;
    text-align:center;
    color:#fff;
}

.requiredInputForm{
	color:#f00;
	margin-right:15px;
}

.popover{
	width:300px;
	height:57px;
	
}

.register .formRegister button a{
	color:#fff;
}

nav .navbar-right{
  line-height:3.5em;
  margin-right:1%;
}

